generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Horario {
  id          Int      @id @default(autoincrement())
  origem      String
  destino     String
  horario     String
  diaDaSemana String
  observacao  String?  // Campo opcional para observações
}
